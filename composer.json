{
  "name": "lucadello91/eloquent-uuid",
  "description": "Laravel Eloquent Model trait for using UUID on primary key",
  "homepage": "https://github.com/lucadello91/eloquent-uuid",
  "license": "MIT",
  "authors": [
    {
      "name": "Luca Dell'Orto",
      "email": "lucadello91@gmail.com"
    }
  ],
  "require": {
    "php": "^7.2 || ^8.0",
    "illuminate/database": "^6.20.26|^7.0|^8.0",
    "illuminate/support": "^6.0|^7.0|^8.0",
    "ramsey/uuid": "^3|^4"
  },
  "require-dev": {
    "illuminate/events": "^6.0|^7.0|^8.0",
    "orchestra/testbench": "^4.0|^5.0|^6.0",
    "phpunit/phpunit": "^9.1"
  },
  "support": {
    "issues": "https://github.com/lucadello91/eloquent-uuid/issues",
    "source": "https://github.com/lucadello91/eloquent-uuid"
  },
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "Lucadello91\\EloquentUuid\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Lucadello91\\EloquentUuid\\Tests\\": "tests"
    }
  }
}
